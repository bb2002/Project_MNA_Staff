<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.saintdev.mnastaff">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name=".models.components.kakao.GlobalApplication"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="kr.saintdev.mnastaff.views.activitys.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".views.activitys.MainActivity"
            android:enabled="true" />

        <activity android:name=".views.activitys.WaitActivity"
            android:enabled="true" />

        <meta-data android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
    </application>
</manifest>